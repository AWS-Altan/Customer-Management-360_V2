<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-ErrorMessageDefaultServer">
    <DisplayName>AM-ErrorMessageDefaultServer</DisplayName>
    <AssignVariable>
        <Name>errorMessage.status</Name>
        <Value>500</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>errorMessage.code</Name>
        <Value>500</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>errorMessage.message</Name>
        <Value>An error has been generated on the server when processing the request. If necessary, please, contact support Altán Redes</Value>
    </AssignVariable>
